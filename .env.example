# API Keys (Optional - public endpoints will be used as fallback)
VITE_ALPHA_VANTAGE_KEY=your_alpha_vantage_key_here
VITE_REDDIT_CLIENT_ID=your_reddit_client_id_here
VITE_REDDIT_CLIENT_SECRET=your_reddit_client_secret_here

# API Endpoints (Optional - defaults will be used if not specified)
VITE_YAHOO_FINANCE_API=https://query1.finance.yahoo.com/v8/finance/chart
VITE_REDDIT_API=https://www.reddit.com
VITE_ALPHA_VANTAGE_API=https://www.alphavantage.co/query

# Application Configuration
VITE_APP_TITLE=Meme vs Market Dashboard
VITE_APP_DESCRIPTION=Correlation analysis between Indian stock market and meme trends
VITE_DEFAULT_TIME_RANGE=30
VITE_CACHE_DURATION=3600000

# Debug and Development
VITE_DEBUG=false
VITE_LOG_LEVEL=error

# MCP Server Configuration
MCP_STOCK_SERVER_PORT=3001
MCP_SOCIAL_SERVER_PORT=3002
FASTMCP_LOG_LEVEL=ERROR

# Performance Settings
VITE_CHART_ANIMATION_DURATION=750
VITE_API_TIMEOUT=10000
VITE_RETRY_ATTEMPTS=3
VITE_RETRY_DELAY=1000

# Feature Flags
VITE_ENABLE_CACHING=true
VITE_ENABLE_OFFLINE_MODE=false
VITE_ENABLE_ANALYTICS=false
